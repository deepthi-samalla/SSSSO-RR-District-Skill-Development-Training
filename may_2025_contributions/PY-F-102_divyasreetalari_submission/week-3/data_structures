def list_num():
    num = list(range(0, 11))
    for number in num:
        print (number)
list_num()

def list1():
    numbers = list(range(1, 11))
    print(numbers)
list1()

def cr_tupl():
    fruits = ('apple','banana','mango','orange','grapes')
    print(fruits[2])
cr_tupl()


def add_numbers_1():
    x = int(input("Enter first number: "))
    y = int(input("Enter second number: "))
    sum = x + y 
    print("The sum is:", sum)
add_numbers_1()

def set_len():
    num = {'1','2','3','4','5'}
    print(len(num))
set_len()    
