    "import pandas as pd\n",
    "\n",
    "# Example 1: Run a SQL query on a Pandas DataFrame\n",
    "df = pd.DataFrame({\"id\": [1, 2, 3], \"value\": [10, 20, 30]})\n",
    "result = duckdb.query(\"SELECT * FROM df WHERE value > 15\").df()\n",
    "print(result)"
