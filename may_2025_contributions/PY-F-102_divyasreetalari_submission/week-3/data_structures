def stud_1():
    students = {}
    for i in range(3):
        name = str(input(f"Enter name of student {i + 1}: "))
        score = int(input(f"Enter score of {name}: "))
        students[name] = score

        print("\nStudent Scores:")
        print(students)
stud_1()

def tuptolist():
    tuple_list=('1','2','3')
    list_1= list(tuple_list)
    print(list_1)
tuptolist()

def list_2():
    string = ['apple','banana','grapes']
    for str in string:
        print(str)
    
list_2()

def names_age():
    emp = {}
    for i in range(3):
        name = str(input(f"Enter name of emp {i + 1}: "))
        age = int(input(f"Enter age of {name}: "))
        emp[name] = age

        print("\nage of emp:")
        print(emp)
names_age()
